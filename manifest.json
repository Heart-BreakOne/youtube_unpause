{
   "manifest_version": 3,
   "name": "PlayNonStop",
   "version": "0.0.2",
   "description": "Skip Youtube autopause and keep playing",
   "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "permissions": [
      "tabs"
   ],
   "content_scripts": [
      {
         "js": [
            "content_script.js"
         ],
         "matches": [
            "https://www.youtube.com/*",
            "https://music.youtube.com/*"
         ]
      }
   ]
}